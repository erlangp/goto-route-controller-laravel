{
	"name": "laravel-route-class-opener",
	"displayName": "Laravel Route Class Opener",
	"description": "Laravel Route Class Opener",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"publisher": "ErlangParasu",
	"activationEvents": [
		"onLanguage:php"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.enableLaravelRouteClassOpener",
				"title": "Enable LaravelRouteClassOpener"
			},
			{
				"command": "extension.openPhpClassFile",
				"title": "Go to PHP Class"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.openPhpClassFile",
					"when": "editorTextFocus",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}